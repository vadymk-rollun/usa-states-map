{
  "name": "vadymk-rollun/usa-states-map",
  "description": "For work with Csv files",
  "type": "library",
  "license": "BSD-3-Clause",
  "require": {
    "php": "8.0.*",
    "rollun-com/rollun-utils": "^8.1.0",
    "ajgl/csv-rfc": "^0.4.0"
  },
  "require-dev": {
    "filp/whoops": "^2.15",
    "phpunit/phpunit": "^9.6.20"
  },
  "autoload": {
    "psr-4": {
      "rollun\\files\\": "src/Files/src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "rollun\\test\\": "test/"
    }
  },
  "scripts": {
    "test": "phpunit --colors=always",
    "test-coverage": "phpunit --colors=always --coverage-clover clover.xml"
  },
  "config": {
    "allow-plugins": {
      "rollun-com/rollun-installer": true
    }
  }
}
